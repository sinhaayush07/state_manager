  },
  "dependencies": {
    "npm": "^6.13.0"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.19",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.22.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.1.4",
