{
  "name": "js_vanilla_state_manager",
  "author": "Ayush Sinha",
  "version": "1.0.0",
  "private": false,
  "description": "This is a lightweigh store for state manangement",
  "keywords": [
    "state-management",
    "store",
    "pubsub",
    "reactive"
  ],
  "homepage": "https://github.com/sinhaayush07/state_manager#readme",
  "bugs": {
    "url": "https://github.com/sinhaayush07/state_manager/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sinhaayush07/state_manager.git"
  },
  "license": "MIT",
  "type": "commonjs",
  "exports": {
    ".": {
      "import": "./dist/state_manager.esm.js",
